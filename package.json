{
  "name": "edurange_refactored",
  "version": "1.0.0",
  "description": "EDURange",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors -p && npm run flask-static-digest",
    "start": "concurrently -n \"WEBPACK,FLASK,CELERY,FLOWER,WEBSSH\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold,bgRed.bold,bgCyan.bold\" \"npm run webpack-watch\" \"npm run flask-server\" \"celery -A edurange_refactored.tasks worker -B -E -f celery.log -l DEBUG\" \"celery -A edurange_refactored.tasks --broker=redis://localhost:6379/0 flower --port=5555\" \"node webssh2/app/index.js\"",
    "webpack-watch": "NODE_ENV=debug webpack --mode development --watch",
    "flask-server": "flask run --host=0.0.0.0",
    "flask-static-digest": "flask digest compile",
    "lint": "eslint \"assets/js/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coojac09/edurange_refactored.git"
  },
  "author": "Jack Cook",
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "bugs": {
    "url": "https://github.com/coojac09/edurange_refactored/issues"
  },
  "homepage": "https://github.com/coojac09/edurange_refactored#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "bootstrap": "^4.5.3",
    "cidr-matcher": "^2.1.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.1",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "morgan": "^1.10.0",
    "popper.js": "^1.15.0",
    "redis": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "socket.io": "^3.0.5",
    "ssh2": "^0.8.9",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/helper-call-delegate": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@json-editor/json-editor": "^2.5.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "concurrently": "^5.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "expose-loader": "^1.0.1",
    "file-loader": "^5.1.0",
    "install": "^0.13.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "npm": "^7.0.10",
    "raw-loader": "^4.0.2",
    "url-loader": "^3.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.2"
  }
}
