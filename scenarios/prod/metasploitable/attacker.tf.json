{
    "resource": [
        {
            "docker_container": [
                {
                    "SNAME_attacker": [
                        {
                            "capabilities": [
                                {
                                    "add": [
                                        "NET_ADMIN",
                                        "SYS_PTRACE"
                                    ]
                                }
                            ],
                            "command": [
                                "/usr/sbin/sshd",
                                "-D"
                            ],
                            "connection": [
                                {
                                  "host": "${self.ports[0].ip}",
                                  "password": "root",
                                  "port": "${self.ports[0].external}",
                                  "type": "ssh",
                                  "user": "root"
                                }
                            ],
                            "hostname": "attacker",
                            "image": "edurange2/metasploit.attacker.dev",
                            "name": "SNAME_attacker",
                            "networks_advanced": [
                                {
                                    "ipv4_address": "OCTET.0.1.3",
                                    "name": "SNAME_NAT"
                                },
                                {
                                    "ipv4_address": "OCTET.0.0.3",
                                    "name": "SNAME_PLAYER"
                                }
                            ],
                            "ports": [
                                {
                                    "internal": 22
                                }
                            ],
                            "host": [
                                {
                                    "host": "target",
                                    "ip": "OCTET.0.0.2"
                                }
                            ],
                            "provisioner": [
                                {
                                    "remote-exec": [
                                        {
                                            "inline": [
                                                "USERS",
                                                "EXECS"
                                            ]
                                        }
                                    ]
                                }
                            ],
                            "restart": "always"
                        }
                    ]
                }
            ]
        }
    ],
    "locals": [
        {
            "SNAME_attacker_extern": "${tostring(docker_container.SNAME_attacker.ports[0].external)}"
        }
    ],
    "output": [
        {
            "SNAME_attacker": [
                {
                    "value": [
                        {
                          "ip_address_public": "${join(\":\", [\"EXTERN_HOST\", local.SNAME_attacker_extern])}",
                          "name": "SNAME_attacker"
                        }
                    ]
                }
            ]
        }
    ]
}
