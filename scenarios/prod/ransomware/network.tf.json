{
	"terraform": {
		"required_providers": {
			"docker": {
				"source": "kreuzwerker/docker"
			}
		}
	},
	"resource": [
		{
			"docker_network": [
				{
					"SNAME_NAT": [
						{
							"driver": "bridge",
							"internal": "false",
							"ipam_config": [
								{
									"subnet": "OCTET.0.1.0/27"
								}
							],
							"name": "SNAME_NAT"
						}
					]
				}
			]
		},
		{
			"docker_network": [
				{
					"SNAME_PLAYER": [
						{
							"driver": "bridge",
							"internal": "true",
							"ipam_config": [
								{
									"subnet": "OCTET.0.0.0/27"
								}
							],
							"name": "SNAME_PLAYER"
						}
					]
				}
			]
		}
	]
}